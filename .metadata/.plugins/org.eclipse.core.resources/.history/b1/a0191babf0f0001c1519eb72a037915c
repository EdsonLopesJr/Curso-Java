package classe;

public class DataTeste {

	public static void main(String[] args) {

		Data d1 = new Data();
		d1.dia = 19;
		d1.mes = "Jun";
		d1.ano = 2022;

		Data d2 = new Data();
		d2.dia = 7;
		d2.mes = "Dez";
		d2.ano = 2022;

		System.out.println(d1.ano+ " "+d1.mes+" "+d1.ano);
		System.out.println(d2.ano+ " "+d2.mes+" "+d2.ano);
		
	}
}
